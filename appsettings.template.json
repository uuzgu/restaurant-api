{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=/app/App_Data/restaurant.db"
  },
  
  "JwtSettings": {
    "SecretKey": "YOUR_JWT_SECRET_KEY",  
    "Issuer": "YOUR_ISSUER_URL",  
    "Audience": "YOUR_AUDIENCE_URL"  
  },

  "Stripe": {
    "SecretKey": "YOUR_STRIPE_SECRET_KEY",
    "WebhookSecret": "YOUR_STRIPE_WEBHOOK_SECRET"
  },

  "FrontendUrl": "https://restaurant-ui-gules.vercel.app"
} 